    <ApplicationIcon />
    <Win32Resource />
    <OutputType>Library</OutputType>
    <StartupObject />

    <PackageId>MessagePack.NodaTime</PackageId>
    <PackageVersion>1.0.0</PackageVersion>

    <PackageProjectUrl>https://github.com/ARKlab/MessagePack</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/ARKlab/MessagePack/blob/master/LICENSE</PackageLicenseUrl>
